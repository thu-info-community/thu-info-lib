{
    "name": "thu-info-lib",
    "version": "1.0.2",
    "description": "The JavaScript library that provides a program-friendly interface to Tsinghua web portal",
    "main": "dist/index.js",
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "play": "node -r esm --experimental-repl-await",
        "demo": "node -r esm demo.js",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "draft": "node versionhelper.js draft",
        "release": "node versionhelper.js release",
        "build-dist": "webpack --config webpack.config.js",
        "watch-dist": "webpack -w --config webpack.config.js"
    },
    "keywords": [
        "tsinghua",
        "thu",
        "info",
        "thuinfo",
        "thu-info",
        "thu info"
    ],
    "maintainers": [
        {
            "name": "UNIDY2002",
            "email": "UNIDY2002@outlook.com"
        },
        {
            "name": "Ashitemaru",
            "email": "qhd19@mails.tsinghua.edu.cn"
        }
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/thu-info-community/thu-info-lib.git"
    },
    "dependencies": {
        "abort-controller": "3.0.0",
        "cheerio": "0.22.0",
        "cross-fetch": "3.1.4",
        "dayjs": "1.10.6",
        "iconv-lite": "0.6.3",
        "md5": "2.3.0",
        "xlsx": "0.17.1"
    },
    "devDependencies": {
        "@types/cheerio": "0.22.30",
        "@types/convert-excel-to-json": "1.7.1",
        "@types/jest": "27.0.1",
        "@types/md5": "2.3.1",
        "@typescript-eslint/eslint-plugin": "4.31.0",
        "@typescript-eslint/parser": "4.31.0",
        "eslint": "7.32.0",
        "esm": "3.2.25",
        "jest": "27.1.1",
        "node-polyfill-webpack-plugin": "1.1.4",
        "semver": "7.3.5",
        "ts-jest": "27.0.5",
        "ts-loader": "9.2.5",
        "typescript": "4.4.2",
        "webpack": "5.52.1",
        "webpack-cli": "4.8.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
